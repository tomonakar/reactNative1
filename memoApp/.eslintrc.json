{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "key-spacing": 0,
    "comma-dangle": 0,
    "semi": 0,
    "object-curly-spacing": 0,
    "no-use-before-define": 0,
    "react/prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-no-bind": 0,
    "react/no-did-mount-set-state": 0
  },
  "parser": "babel-eslint"
}
